{
  "version": 2,
  "routes": [
    { "src": "/api/auth", "dest": "/api/auth.js" },
    { "src": "/api/verify", "dest": "/api/verify.js" },
    { "src": "/sistema.html", "dest": "/api/protected?sistema.html" },
    { "src": "/pagar.html", "dest": "/api/protected?pagar.html" },
    { "src": "/filme.html", "dest": "/api/protected?filme.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
